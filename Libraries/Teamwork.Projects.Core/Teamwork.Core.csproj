<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <RootNamespace>Teamwork.Projects</RootNamespace>
    <ApplicationIcon />
    <StartupObject />
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>0.9.3.0</AssemblyVersion>
    <FileVersion>0.9.3.0</FileVersion>
    <PackageReleaseNotes>Updated UserAgent 
</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/Teamwork/dotnet</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <Copyright>http://www.teamwork.com</Copyright>
    <PackageProjectUrl>https://github.com/Teamwork/dotnet</PackageProjectUrl>
    <Description>SDK to work with Teamwork API's</Description>
    <Authors>Tim Cadenbach</Authors>
    <Company>Teamwork.com</Company>
    <Product>Teamwork Projects</Product>
    <Version>0.9.3</Version>
    <PackageId>Teamwork.Core</PackageId>
    <PackageIconUrl>https://avatars3.githubusercontent.com/u/4037476?s=200&amp;v=4</PackageIconUrl>
    <PackageLicenseUrl>https://www.teamwork.com</PackageLicenseUrl>
    <PackageTags>Teamwork; Projects;</PackageTags>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.1" />
  </ItemGroup>


  <Import Project="..\Teamwork.Shared.Schema\Teamwork.Shared.Schema.projitems" Label="Shared" />


  <Import Project="..\Teamwork.Shared.Common\Teamwork.Shared.Common.projitems" Label="Shared" />

